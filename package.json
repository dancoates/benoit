    "whiteListedModules": [
      "react-enty",
      "styled-components",
      "react-monaco-editor"
    ]
  },
  "dependencies": {
    "better-sqlite3": "^5.4.3",
    "csv-parser": "^2.3.2",
    "dataparcels": "^1.0.2",
    "debounce": "^1.2.0",
    "highland": "^2.13.5",
    "monaco-editor": "^0.20.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-enty": "^1.0.0-alpha.8",
    "react-is": "^16.12.0",
    "react-monaco-editor": "^0.34.0",
    "react-splitter-layout": "^4.0.0",
    "react-table": "^7.0.0-rc.15",
    "react-window": "^1.8.5",
    "react-window-infinite-loader": "^1.0.5",
    "rxjs": "^6.5.4",
    "source-map-support": "^0.5.12",
