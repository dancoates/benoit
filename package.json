    "@types/node": "12",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-splitter-layout": "^3.0.0",
    "@types/styled-components": "^4.4.3",
    "@types/uuid": "^3.4.7",
    "electron": "8.0.0",
    "electron-builder": "^22.3.2",
    "electron-webpack": "^2.7.4",
    "electron-webpack-ts": "^3.2.0",
    "monaco-editor-webpack-plugin": "^1.9.0",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5",
    "webpack": "~4.41.5"
